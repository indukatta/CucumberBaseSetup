</listeners>
<test name="Set up" verbose="2">
    <classes>
        <class name= "Onboarding_tests.SetUpTests"/>
    </classes>
</test>
    <test name="Business search" verbose="2">
        <classes>
            <class name= "Onboarding_tests.businessSearchTests"/>
        </classes>
    </test>
    <test name="Personal details" verbose="2">
        <classes>
            <class name= "Onboarding_tests.personalDetailsTests"/>
        </classes>
    </test>
    <test name="Business form" verbose="2">
        <classes>
            <class name= "Onboarding_tests.businessFormTests"/>
        </classes>
    </test>
    <test name="Ongoing source of wealth" verbose="2">
        <classes>
            <class name= "Onboarding_tests.OngoingSourceOfWealthTests"/>
        </classes>
    </test>
    <test name="End-to-end" verbose="2">
        <classes>
            <class name= "Onboarding_tests.endToEndTests"/>
        </classes>
    </test>
