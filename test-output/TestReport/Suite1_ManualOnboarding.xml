<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Release 1">
<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
</listeners>
<test name="Manual Onboarding - Set up" verbose="2">
    <classes>
        <class name= "Onboarding_tests.SetUpTests"/>
    </classes>
</test>
    <test name="Manual Onboarding - Business search" verbose="2">
        <classes>
            <class name= "Onboarding_tests.businessSearchTests"/>
        </classes>
    </test>
    <test name="Manual Onboarding - Personal details" verbose="2">
        <classes>
            <class name= "Onboarding_tests.personalDetailsTests"/>
        </classes>
    </test>
    <test name="Manual Onboarding - Business form" verbose="2">
        <classes>
            <class name= "Onboarding_tests.businessFormTests"/>
        </classes>
    </test>
    <test name="Manual Onboarding - Ongoing source of wealth" verbose="2">
        <classes>
            <class name= "Onboarding_tests.OngoingSourceOfWealthTests"/>
        </classes>
    </test>
    <test name="Manual Onboarding - End-to-end" verbose="2">
        <classes>
            <class name= "Onboarding_tests.endToEndTests"/>
        </classes>
    </test>
    <test name="Login / Logout" verbose="2">
        <classes>
            <class name= "Login.loginTests"/>
        </classes>
    </test>
</suite>